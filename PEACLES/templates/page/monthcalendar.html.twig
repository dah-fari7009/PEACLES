{% set tab = dates(start,month,year) %}
<div class="row">
    <p> {{ month }} {{ year }} </p>
</div>
<div class="row">

    {% for key,value in tab %}
        <div class="col">
            <p> {{ key }} </p>
            {% for day in value %}
                {% if day<=7 and index(key,tab)+1<day %}
                    <p><br></p>
                {% endif %}
                 <p> {{ day }} </p>
                
            {% endfor %}
        </div>
    {% endfor %}
</div>